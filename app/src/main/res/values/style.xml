<?xml version="1.0" encoding="utf-8"?>
<resources>
<!-- Base application theme. -->
<style name="Green.Base" parent="Theme.AppCompat.Light.NoActionBar">

    <item name="colorPrimary">@color/colorPrimaryGreen</item>

    <item name="colorPrimaryDark">@color/colorPrimaryDarkGreen</item>

    <item name="colorAccent">@color/colorAccentGreen</item>





</style>

    <style name="Blue.Base" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="colorPrimary">@color/colorPrimaryBlue</item>

        <item name="colorPrimaryDark">@color/colorPrimaryDarkBlue</item>

        <item name="colorAccent">@color/colorAccentBlue</item>
        <item name="android:windowBackground">?attr/colorBackgroundFloating</item>

    </style>

    <style name="Red.Base" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="colorPrimary">@color/colorPrimaryRed</item>

        <item name="colorPrimaryDark">@color/colorPrimaryDarkRed</item>

        <item name="colorAccent">@color/colorAccentRed</item>

    </style>

    <style name="Yellow.Base" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="colorPrimary">@color/colorPrimaryYellow</item>

        <item name="colorPrimaryDark">@color/colorPrimaryDarkYellow</item>

        <item name="colorAccent">@color/colorAccentYellow</item>

    </style>

    <style name="DayNight.Base" parent="Theme.AppCompat.DayNight.NoActionBar">

        <item name="colorPrimary">@color/gray</item>

        <item name="colorPrimaryDark">@color/darkgray</item>

        <item name="colorAccent">@color/acgray</item>

    </style>

<style name="myDialog" parent="Dialog">
    

    <item name="android:windowFrame">@null</item>
    <item name="windowActionBarOverlay">true</item>
    <item name="windowActionModeOverlay">true</item>
    <item name="actionBarPopupTheme">@null</item>
    <item name="windowNoTitle">false</item>
    <item name="android:textColor">@color/colorAccentYellow</item>
    <item name="windowActionBar">false</item>
    <item name="android:textColorPrimary">@color/colorPrimaryDarkGreen</item>





</style>

    <style name="myET" parent="Base.V7.Widget.AppCompat.EditText">

    </style>

    <style name="Dialog" parent="Theme.AppCompat.Light.Dialog.Alert">

        <item name="android:textColorPrimary">@color/white</item>


           </style>

</resources>